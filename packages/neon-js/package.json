{
  "name": "@cityofzion/neon-js",
  "description": "Neon-JS SDK for interacting with NEO blockchain",
  "version": "4.0.0-next.1",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CityOfZion/neon-js.git"
  },
  "keywords": [
    "neo",
    "antshares",
    "javascript",
    "libraries"
  ],
  "author": "Yak Jun Xiang <snowypowers@gmail.com> (https://github.com/snowypowers)",
  "license": "MIT",
  "main": "dist/index.js",
  "browser": "dist/browser.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dist": "cross-env NODE_ENV=production webpack --mode production",
    "dist:dev": "cross-env NODE_ENV=development webpack --mode development",
    "dist:js": "tsc && cross-env NODE_ENV=development webpack --mode production -c ./webpack.lib.config.js",
    "lint": "tslint src/**/*.ts",
    "lint:fix": "tslint src/**/*.ts --fix",
    "prepublishOnly": "yarn lint && yarn build",
    "pretty": "prettier",
    "clean": "rimraf ./lib ./dist"
  },
  "dependencies": {
    "@cityofzion/neon-api": "^4.0.0-next.1",
    "@cityofzion/neon-core": "^4.0.0-next.1",
    "@cityofzion/neon-nep5": "^4.0.0-next.1"
  },
  "devDependencies": {
    "@types/jest": "^23.3.1",
    "clean-webpack-plugin": "^0.1.19",
    "cross-env": "^5.2.0",
    "jest": "^23.4.2",
    "prettier": "^1.14.0",
    "rimraf": "^2.6.2",
    "ts-jest": "^23.0.1",
    "ts-loader": "^4.4.2",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.14.0",
    "typescript": "^3.0.1",
    "webpack": "^4.16.3",
    "webpack-cli": "^3.1.0"
  },
  "files": [
    "dist/"
  ]
}
