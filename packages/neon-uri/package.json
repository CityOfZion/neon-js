  "version": "5.0.0-next.13",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CityOfZion/neon-js.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "neo",
    "antshares",
    "javascript",
